{% extends 'base.html.twig' %}

{% block title %}Créer une sortie{% endblock %}

{% block body %}
    <h1>Créer une sortie</h1>

    {{ form_start(sortieForm) }}
    {{ form_widget(sortieForm) }}
    <a href="{{ path('sortie_nouveau_lieu') }}">Ajouter lieu</a>
    {{ form_end(sortieForm) }}
    <button class="btn btn-danger" type="button" name="annuler" onclick="history.go(0)">Annuler</button>
{% endblock %}